{
  "name": "quickstart-api",
  "version": "0.0.1",
  "description": "Quickstart API",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "posttest": "npm run lint",
    "docker:build": "docker build -t quickstart-api .",
    "docker:run": "docker run -p 3000:3000 -d quickstart-api",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build",
    "esprimi": "node ./src/utils/esprimi-api-cli/esprimi-api-main.js",
    "test": "mocha --recursive \"dist/__tests__\"",
    "start:watch": "tsc-watch --target es2017 --outDir ./dist --onSuccess \"node .\"",
    "seed": "node src/utils/seed/seed-module-permission.js"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Alexis Barros <apenasalexis@gmail.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@google-cloud/storage": "^6.5.2",
    "@loopback/authentication": "^9.0.9",
    "@loopback/authentication-jwt": "^0.12.10",
    "@loopback/boot": "^5.0.9",
    "@loopback/core": "^4.0.9",
    "@loopback/repository": "^5.1.4",
    "@loopback/rest": "^12.0.9",
    "@loopback/rest-explorer": "^5.0.9",
    "@loopback/security": "^0.8.9",
    "@loopback/service-proxy": "^5.0.9",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/multer": "^1.4.7",
    "@types/node-fetch": "^3.0.3",
    "axios": "^0.24.0",
    "dotenv": "^10.0.0",
    "googleapis": "^92.0.0",
    "jsonwebtoken": "^8.5.1",
    "loopback-boot": "^3.3.1",
    "loopback-connector-mongodb": "^6.1.0",
    "mongodb": "^4.5.0",
    "mongoose": "^6.6.1",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.6.6",
    "nodemailer": "^6.7.2",
    "pluralize": "^8.0.0",
    "query-string": "^7.0.1",
    "tslib": "^2.0.0",
    "tslog": "^3.3.1"
  },
  "devDependencies": {
    "@loopback/build": "^9.0.9",
    "@loopback/eslint-config": "^13.0.9",
    "@loopback/testlab": "^5.0.9",
    "@supercharge/strings": "^1.25.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^14.18.36",
    "@types/nodemailer": "^6.4.4",
    "@types/pluralize": "0.0.29",
    "eslint": "^8.35.0",
    "fs-extra": "^10.1.0",
    "lodash": "^4.17.21",
    "mocha": "^10.2.0",
    "source-map-support": "^0.5.21",
    "tsc-watch": "^5.0.3",
    "typescript": "~4.9.4"
  }
}
